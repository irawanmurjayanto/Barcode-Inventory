<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/colorBlue"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp"
    tools:context=".About">
    <TextView
        android:padding="5dp"
        android:textColor="@color/colorWhite"
        android:background="@color/colorAccent"

    android:id="@+id/textID1"
    android:gravity="center"
    android:text="Barcode Sawmill PT. KINI Paradaise 2021 versi 1.1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" />

    <TextView
        android:padding="5dp"
        android:textColor="@color/colorWhite"
        android:background="@color/colorAccent"
        android:layout_below="@+id/textID1"

        android:id="@+id/textID2"
        android:gravity="center"
        android:text="Bisa digunakan untuk android Ice Cream Ke atas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:padding="5dp"
        android:textColor="@color/colorWhite"
        android:background="@color/colorAccent"

        android:layout_below="@+id/textID2"
        android:id="@+id/textID3"
        android:gravity="center"
        android:text="(Android Developer/Irawan Murjayanto)"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</RelativeLayout>