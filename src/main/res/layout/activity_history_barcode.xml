<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".HistoryBarcode">


    <TextView
        android:layout_marginTop="20dp"
        android:layout_marginLeft="5dp"
        android:textColor="@color/colorWhite"
        android:textSize="8dp"
        android:padding="2dp"
        android:background="@color/colorBlack"
        android:text="Barcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <EditText
        android:id="@+id/editbarcode"
        android:layout_width="200dp"
        android:layout_height="wrap_content"/>
        <Button
            android:textColor="@color/colorWhite"
            android:id="@+id/butprocess"
            android:layout_toRightOf="@+id/editbarcode"
            android:background="@drawable/addround"
            android:textSize="10dp"
            android:text="Process"
            android:layout_width="70dp"
            android:layout_height="40dp"/>
        <Button
            android:layout_marginLeft="10dp"
            android:id="@+id/butbarcode"
            android:layout_toRightOf="@+id/butprocess"
            android:background="@drawable/barcode2"
            android:textSize="8dp"
            android:layout_width="70dp"
            android:layout_height="40dp"/>
    </RelativeLayout>
    <WebView
        android:id = "@+id/web_view"
        android:layout_width = "match_parent"
        android:layout_height = "match_parent" />
</LinearLayout>